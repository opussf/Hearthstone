<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.blizzard.com/wow/ui/">
	<!-- Version 2.0 -->
	<Frame name="HSFrame"  frameStrata="BACKGROUND" toplevel="false" parent="UIParent">
		<Scripts>
			<OnLoad>
				HS.OnLoad()
			</OnLoad>
			<OnEvent>
				HS[event](self, ...)
			</OnEvent>
		</Scripts>
	</Frame>
	<Frame name="HSConfig" toplevel="false" parent="UIParent" enableMouse="true" movable="true" resizable="false">
		<Size x="400" y="400" />
		<Anchors>
			<Anchor point="CENTER"/>
		</Anchors>
		<Layers>
		</Layers>
	</Frame>
</Ui>
